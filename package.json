{
  "name": "kcwiki-quest-data",
  "version": "0.6.1",
  "description": "Kancolle Quest Data",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run generateSchema && npm run buildData && npm run translate",
    "generateSchema": "mkdir -p build && quicktype --src types/index.ts --src-lang ts --lang schema --out build/schema-quicktype.json && ts-node scripts/tweak-schema.ts",
    "buildData": "ts-node scripts/build-data.ts",
    "translate": "ts-node translator/index.ts",
    "tsc": "tsc",
    "build": "npm run generateSchema && npm run buildData && npm run translate && npm run tsc",
    "checkPoi": "ts-node scripts/check-quest.ts",
    "format": "prettier --ignore-path .gitignore .",
    "format:check": "npm run format -- --check",
    "format:fix": "npm run format -- --write",
    "clean": "rm -rf build/* dist",
    "test": "jest",
    "test:ci": "jest --ci --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kcwikizh/kcwiki-quest-data.git"
  },
  "keywords": [
    "Kancolle",
    "Kantai",
    "Collection"
  ],
  "author": "kcwikizh",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "@types/node-fetch": "^2.5.7",
    "ajv": "^6.12.0",
    "csvtojson": "^2.0.10",
    "fs-extra": "^9.0.0",
    "i18next": "^19.4.4",
    "inflection": "^1.12.0",
    "jest": "^25.2.4",
    "node-fetch": "^2.6.0",
    "poi-plugin-translator": "latest",
    "prettier": "^2.0.2",
    "quicktype": "^15.0.220",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
