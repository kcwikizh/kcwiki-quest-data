{
  "name": "kcwiki-quest-data",
  "version": "0.2.0",
  "description": "Kancolle Quest Data",
  "scripts": {
    "start": "npm run generateSchema && npm run build",
    "generateSchema": "mkdir -p build && quicktype --src types/index.ts --src-lang ts --lang schema --out build/schema-quicktype.json && ts-node scripts/tweak-schema.ts",
    "build": "ts-node scripts/build.ts",
    "postinstall": "npm run generateSchema",
    "format": "prettier --ignore-path .gitignore .",
    "format:check": "npm run format -- --check",
    "format:fix": "npm run format -- --write",
    "clean": "rm -rf build/*",
    "test": "jest",
    "test:ci": "jest --ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kcwikizh/kcwiki-quest-data.git"
  },
  "keywords": [
    "Kancolle",
    "Kantai",
    "Collection"
  ],
  "author": "kcwikizh",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "ajv": "^6.12.0",
    "jest": "^25.2.4",
    "lodash": "^4.17.15",
    "prettier": "^2.0.2",
    "quicktype": "^15.0.220",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
